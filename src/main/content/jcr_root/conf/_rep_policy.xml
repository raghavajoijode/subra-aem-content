<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:crx="http://www.day.com/crx/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:rep="internal"
    jcr:primaryType="rep:ACL">
    <allow65
        jcr:primaryType="rep:GrantACE"
        rep:principalName="forms-users"
        rep:privileges="{Name}[jcr:read]">
        <rep:restrictions
            jcr:primaryType="rep:Restrictions"
            rep:glob="/*/dam/*"/>
    </allow65>
	<allow-sadmins
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sadmins"
        rep:privileges="{Name}[jcr:versionManagement,jcr:read,rep:write,jcr:lockManagement]"/>
     <allows-authors
        jcr:primaryType="rep:GrantACE"
        rep:principalName="sauthors"
        rep:privileges="{Name}[jcr:versionManagement,jcr:modifyProperties,jcr:read,rep:write,jcr:lockManagement]"/>
</jcr:root>
